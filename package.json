{
  "name": "yumemi-code-check",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
    "format": "eslint --fix './**/*.{js,jsx,ts,tsx}'",
    "preview": "vite preview",
    "preinstall": "typesync || :",
    "prepare": "husky install"
  },
  "dependencies": {
    "@linaria/babel-preset": "^3.0.0-beta.15",
    "@linaria/core": "^3.0.0-beta.15",
    "@linaria/react": "^3.0.0-beta.15",
    "@linaria/rollup": "^3.0.0-beta.15",
    "@linaria/shaker": "^3.0.0-beta.15",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "chart.js": "^3.7.1",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.1",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "typescript": "^4.5.4",
    "typesync": "^0.8.0",
    "vite": "^2.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix ./**/*.{js,jsx,ts,tsx}'"
    ]
  }
}
